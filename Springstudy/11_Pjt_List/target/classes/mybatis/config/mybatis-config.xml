	<typeAliases>
		<typeAlias type="com.gdu.app11.domain.EmpDTO" alias="EmpDTO"/>  
		<typeAlias type="com.gdu.app11.domain.DeptDTO" alias="DeptDTO"/>  
	</typeAliases>
	
</configuration>
